{
  "name": "simbex-reload-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --project './tsconfig.json'",
    "dev": "tsc --project './tsconfig.json' --watch & NODE_ENV=development nodemon dist/bin/www.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "docker:build-local": "docker build -t simbex-reload-api:local .",
    "docker:run-local": "docker run -e 'NODE_ENV=development' --env-file='.env' -it --rm -p 8080:80 simbex-reload-api:local",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hapi/joi": "^14.5.0",
    "@types/cors": "^2.8.7",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "http-errors": "^1.8.0",
    "kue": "^0.11.6",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.1",
    "swagger-jsdoc": "^6.0.0-rc.5",
    "swagger-ui-express": "^4.1.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.4",
    "@types/http-errors": "^1.6.3",
    "@types/kue": "^0.11.13",
    "@types/lodash": "^4.14.157",
    "@types/morgan": "^1.9.0",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.2.10",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
